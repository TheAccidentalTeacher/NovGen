{
  "name": "novgen-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/api/project/[id]/chapters/route.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "MONGODB_URI": "@mongodb_uri",
    "DEBUG_LOGS": "true"
  }
}
